##Q2
y =c(132,143,153,162,154,168,137,149,159,128,166)
x1=c(52,59,67,73,64,74,54,61,65,46,72)
x2=c(173,184,194,211,196,220,188,188,207,167,217)
model =lm(y~x1+x2);model

names(model)
#b)
residual=model$residuals;residual
var(residual)

##c)
summary(model)

##d)
par(mfrow=c(2,2))




plot(residual)

